{
  "name": "miranda-dash",
  "version": "1.0.0",
  "description": "Keeps track of moves and allows user to create move estimates",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
