    "bcryptjs": "^2.4.3",
    "config": "^3.2.2",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.6",
    "react-awesome-modal": "^2.0.5",
    "sass": "^1.83.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  },
  "engines": {
    "node": "20.x",
    "npm": "11.x"
  }
